Proceso restaEnteros
	Definir valor1, valor2, resultado Como Entero;
	Escribir 'Bienvenido, por favor ingresar valores';
	Escribir 'Ingresar el primer valor';
	Leer valor1;
	Escribir 'Ingresar el segundo valor';
	Leer valor2;
	resultado <- valor1-valor2;
	Escribir 'La resta de los valores es: ', resultado;
FinProceso
