Proceso conversionDolaresCordobas
	Definir cordobas, cantDolares, conversion Como Real;
	Escribir "Bienvenido, para llevar a cabo la conversión de dólares a córdobas, completa los siguientes datos";
	Escribir "Ingresa el valor del córdoba actual";
	Leer cordobas;
	Escribir "Ingresa la cantidad de dólares";
	Leer cantDolares;
	conversion <- cantDolares * cordobas;
	Escribir "La cantidad de ", cantDolares, "  dólares a córdobas es ", conversion; 
FinProceso
