Proceso divisionEnteros
	Definir valor1, valor2, resultado Como Entero;
	Escribir "Bienvenido, por favor ingresar los valores";
	Escribir "Ingresar el primer valor";
	Leer valor1;
	Escribir "Ingresar el segundo valor";
	Leer valor2;
	resultado <- valor1 / valor2;
	Escribir "El resultado de la división es: ", resultado;
FinProceso
