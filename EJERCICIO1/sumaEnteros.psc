Proceso sumaEnteros
	Definir valor1, valor2, resultado Como Entero;
	Escribir 'Bienvenido, por favor ingresar los valores';
	Escribir 'Ingrese el primer valor';
	Leer valor1;
	Escribir 'Ingrese el segundo valor';
	Leer valor2;
	resultado <- valor1+valor2;
	Escribir 'El resultado de la suma de los valores es: ', resultado;
FinProceso
